{
    "name": "piksel-nl/Kigo-Pro-API-Client",
    "description": "This PHP Client has support for Kigo Pro API's",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "kigo",
        "pro",
        "channels-v2"
    ],
    "homepage": "https://piksel.nl",
    "license": "unlicense",
    "authors": [
        {
            "name": "Piksel",
            "homepage": "https://piksel.nl"
        },
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Piksel\\Kigo\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Piksel\\Kigo\\Test\\" : "test/" }
    }
}
